{
  "kind": "collectionType",
  "collectionName": "albums",
  "info": {
    "name": "Albums",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "songs": {
      "via": "album",
      "collection": "song"
    },
    "artist": {
      "model": "artist"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "under_review",
        "pubblished",
        "denied"
      ],
      "default": "under_review",
      "private": true
    },
    "stock": {
      "type": "biginteger"
    },
    "cover": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    }
  }
}
